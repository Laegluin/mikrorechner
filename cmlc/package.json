{
	"name": "cml",
	"displayName": "cml",
	"description": "Language support for cml",
	"version": "0.1.0",
	"publisher": "laegluin",
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "cml",
				"extensions": [
					".cml"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cml",
				"scopeName": "source.cml",
				"path": "./grammars/cml.tmLanguage.json"
			}
		]
	}
}